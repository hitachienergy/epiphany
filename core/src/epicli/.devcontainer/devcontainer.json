{
    "name": "epicli",
    "dockerFile": "Dockerfile",
    "extensions": [
        "ms-python.python",
        "littlefoxteam.vscode-python-test-adapter",
        "redhat.ansible",
        "samuelcolvin.jinjahtml",
        "redhat.vscode-yaml",
        "mauve.terraform",
        "davidanson.vscode-markdownlint",
        "timonwong.shellcheck",
        "shardulm94.trailing-spaces",
    ],
    "settings": {
        "python.pythonPath": "/usr/local/bin/python"
    },
    "runArgs": [ "-u", "vscode" ]
}
